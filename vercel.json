{
  "version": 2,
  "builds": [
    {
      "src": "api/lastfm.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/youtubeSearch.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/lastfm",
      "dest": "/api/lastfm.js"
    },
    {
      "src": "/api/youtubeSearch",
      "dest": "/api/youtubeSearch.js"
    }
  ]
}